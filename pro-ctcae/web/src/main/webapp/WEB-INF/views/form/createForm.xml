<flow xmlns="http://www.springframework.org/schema/webflow"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="
		http://www.springframework.org/schema/webflow
		http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">


    <var name="createFormCommand" class="gov.nih.nci.ctcae.web.form.CreateFormCommand"></var>

    <view-state id="createForm" view="form/createForm">
        <on-render>
            <evaluate expression="proCtcTermRepository.findAll()" result="viewScope.proCtcTerms"></evaluate>
        </on-render>
    </view-state>
</flow>